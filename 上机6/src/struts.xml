<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
   



   <package name="user" extends="struts-default">
   
       <!-- class="RegistAction"必须与applicationContext.xml中的id对应 -->
         
       <action name ="checkregist" class = "com.hjs.action.RegistAction">
           <result name="success">/successregister.jsp</result>
           <result name="input">/failregister.jsp</result>
       </action>
       
       <action name = "checklogin" class = "com.hjs.action.LoginAction">
           <result name="success">/SearchTicket.jsp</result>
           <result name="input">/Register.jsp</result>
           <result name="login">/admin.jsp</result>
       </action>
   </package>

</struts>

   
